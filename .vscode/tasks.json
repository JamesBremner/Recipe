{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "recipe",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "command": "mingw32-make.exe",
            "args": [
                "-f",
                ".vscode/makefile",
                "recipe"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        },
        {
            "label": "rebuild",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "command": "mingw32-make.exe",
            "args": [
                "-f",
                ".vscode/makefile",
                "rebuild"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        },
        {
            "label": "clean",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "command": "mingw32-make.exe",
            "args": [
                "-f",
                ".vscode/makefile",
                "clean"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "C:\\msys64\\ucrt64\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-o",
                "${workspaceFolder}\\bin\\recipe.exe",
                "${workspaceFolder}\\.vscode\\obj\\cRecipeGUI.o",
                "${workspaceFolder}\\.vscode\\obj\\cVase.o",                
                "${workspaceFolder}\\.vscode\\obj\\flower.o",                
                "${workspaceFolder}\\.vscode\\obj\\flower_types.o",                
                "${workspaceFolder}\\.vscode\\obj\\cQuality.o",               
                "${workspaceFolder}\\.vscode\\obj\\raven_sqlite.o",               
                "${workspaceFolder}\\.vscode\\obj\\sqlite3.o",               
                "${workspaceFolder}\\.vscode\\obj\\cxy.o",
                "-I",
                "${workspaceFolder}/src",
                "-I",
                "${workspaceFolder}/src/ext",
                "-I",
                "${workspaceFolder}/models",
                "-I",
                "${workspaceFolder}/models/basic",
                "-I",
                "${workspaceFolder}/../windex/include",
                "-I",
                "${workspaceFolder}/../raven-set",
                "-I",
                "${workspaceFolder}/../../code_ext/boost/boost1_78",
                "-lgdiplus",
                "-lgdi32",
                "-lcomdlg32",
                "-lstdc++fs",
                "-lComctl32"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}